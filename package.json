{
    "name": "hexedit",
    "version": "0.3.0",
    "scripts": {
        "build": "webpack",
        "dev": "cross-env NODE_ENV=development npm run build && cross-env NODE_ENV=development npm start",
        "start": "electron .",
        "postinstall": "npm run build",
        "dist": "npm run build && electron-builder -p never --win"
    },
    "devDependencies": {
        "@babel/core": "7.12.10",
        "@babel/plugin-proposal-class-properties": "7.12.1",
        "@babel/preset-env": "7.12.11",
        "@babel/preset-react": "7.12.10",
        "babel-loader": "8.2.2",
        "cross-env": "^7.0.3",
        "css-loader": "5.0.1",
        "electron": "15.0.0",
        "electron-builder": "^22.11.7",
        "file-loader": "6.2.0",
        "html-webpack-plugin": "5.2.0",
        "style-loader": "2.0.0",
        "webpack": "5.24.2",
        "webpack-cli": "4.5.0"
    },
    "dependencies": {
        "classnames": "^2.3.1",
        "express": "4.17.1",
        "prop-types": "15.7.2",
        "react": "17.0.1",
        "react-dom": "17.0.1",
        "uuid": "^8.3.2"
    },
    "build": {
        "win": {
            "target": [
                "portable"
            ]
        },
        "files": [
            "build",
            "index.js",
            "server"
        ]
    }
}
